{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "tui",
  "tasks": {
    "ready": {
      "dependsOn": ["^ready"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "dependsOn": ["ready"],
      "outputs": ["dist/**", "build/**"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build", "^ready"],
      "outputs": ["../../dist/**", "dist/**", "build/**"],
      "cache": false
    },
    "e2e": {
      "cache": false
    },
    "type-check": {},
    "lint": {},
    "lint:fix": {},
    "format": {},
    "clean:node_modules": {
      "dependsOn": ["^clean:node_modules"],
      "cache": false
    },
    "clean:turbo": {
      "dependsOn": ["^clean:turbo"],
      "cache": false
    },
    "clean:bundle": {
      "dependsOn": ["^clean:bundle"],
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    }
  }
}
